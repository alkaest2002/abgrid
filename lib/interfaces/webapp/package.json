{
  "name": "abgrid-client",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 53472",
    "build": "vite build --emptyOutDir",
    "build:electron:thin": "npm run build && npm run copy:dist:electron:thin && npm run copy:app:electron:thin",
    "copy:dist:electron:thin": "rm -rf ../electron/thin/app/dist && cp -r dist ../electron/thin/app/dist",
    "copy:app:electron:thin": "rm -rf ../electron/Electron.app/Contents/Resources/app &&  cp -r ../electron/thin/app ../electron/Electron.app/Contents/Resources/app",
    "build:electron:full": "npm run build && npm run copy:dist:electron:full && npm run copy:app:electron:full",
    "copy:dist:electron:full": "rm -rf ../electron/full/app/dist && cp -r dist ../electron/full/app/dist",
    "copy:app:electron:full": "rm -rf ../electron/Electron.app/Contents/Resources/app &&  cp -r ../electron/full/app ../electron/Electron.app/Contents/Resources/app",
    "preview": "vite preview"
  },
  "devDependencies": {
    "vite": "^7.2.4"
  },
  "dependencies": {
    "@alpinejs/persist": "^3.15.2",
    "@tailwindcss/vite": "^4.1.17",
    "@unseenco/taxi": "^1.9.1",
    "alpinejs": "^3.15.2",
    "alpinejs-i18n": "^2.5.3",
    "tailwindcss": "^4.1.17",
    "yaml": "^2.8.1"
  }
}
